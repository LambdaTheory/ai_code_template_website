{
  "blog": {
    "title": "{{ website_title }}博客 - 前端开发技术分享",
    "description": "{{ website_description }}",
    "keywords": ["前端开发", "AI工具", "代码模板", "React", "Next.js", "TypeScript", "技术分享"],
    "author": "{{ author_name }}",
    "siteUrl": "https://{{ website_url }}",
    "openGraph": {
      "type": "website",
      "siteName": "{{ website_title }}博客",
      "locale": "zh_CN",
      "images": [
        {
          "url": "/images/blog-og-image.jpg",
          "width": 1200,
          "height": 630,
          "alt": "AI代码模板博客"
        }
      ]
    },
    "twitter": {
      "card": "summary_large_image",
      "site": "{% if twitter_handle %}@{{ twitter_handle }}{% else %}@{{ website_url.split('.')[0] }}{% endif %}",
      "creator": "{% if twitter_handle %}@{{ twitter_handle }}{% else %}@{{ website_url.split('.')[0] }}{% endif %}"
    },
    "structuredData": {
      "website": {
        "@context": "https://schema.org",
        "@type": "WebSite",
        "name": "{{ website_title }}博客",
        "url": "https://{{ website_url }}",
        "description": "分享前端开发、AI工具和代码模板的最新技术文章",
        "potentialAction": {
          "@type": "SearchAction",
          "target": "https://{{ website_url }}/search?q={search_term_string}",
          "query-input": "required name=search_term_string"
        }
      },
      "organization": {
        "@context": "https://schema.org",
        "@type": "Organization",
        "name": "{{ website_title }}",
        "url": "https://{{ website_url }}",
        "logo": {
          "@type": "ImageObject",
          "url": "https://{{ website_url }}/logo.png"
        },
        "contactPoint": {
          "@type": "ContactPoint",
          "contactType": "客户服务",
          "email": "{{ author_email }}"
        }
      },
      "blog": {
        "@context": "https://schema.org",
        "@type": "Blog",
        "name": "{{ website_title }}博客",
        "description": "{{ website_description }}",
        "url": "https://{{ website_url }}/blog",
        "author": {
          "@type": "Organization",
          "name": "{{ author_name }}"
        },
        "publisher": {
          "@type": "Organization",
          "name": "AI代码模板",
          "logo": {
            "@type": "ImageObject",
            "url": "https://{{ website_url }}/logo.png"
          }
        }
      },
      "article": {
        "@context": "https://schema.org",
        "@type": "BlogPosting",
        "headline": "{title}",
        "description": "{description}",
        "image": "{image}",
        "author": {
          "@type": "Person",
          "name": "{author}"
        },
        "publisher": {
          "@type": "Organization",
          "name": "AI代码模板",
          "logo": {
            "@type": "ImageObject",
            "url": "https://{{ website_url }}/logo.png"
          }
        },
        "datePublished": "{datePublished}",
        "dateModified": "{dateModified}",
        "mainEntityOfPage": {
          "@type": "WebPage",
          "@id": "{url}"
        },
        "keywords": "{keywords}",
        "articleSection": "{category}",
        "wordCount": "{wordCount}"
      }
    }
  },
  "defaults": {
    "title": "{{ website_title }}",
    "description": "{{ website_description }}",
    "keywords": ["AI", "代码模板", "前端开发", "React", "Next.js", "开发工具"],
    "author": "{{ author_name }}",
    "siteUrl": "https://{{ website_url }}",
    "openGraph": {
      "type": "website",
      "siteName": "{{ website_title }}",
      "locale": "zh_CN",
      "images": [
        {
          "url": "/images/og-image.jpg",
          "width": 1200,
          "height": 630,
          "alt": "AI代码模板"
        }
      ]
    },
    "twitter": {
      "card": "summary_large_image",
      "site": "{% if twitter_handle %}@{{ twitter_handle }}{% else %}@{{ website_url.split('.')[0] }}{% endif %}",
      "creator": "{% if twitter_handle %}@{{ twitter_handle }}{% else %}@{{ website_url.split('.')[0] }}{% endif %}"
    }
  }
}